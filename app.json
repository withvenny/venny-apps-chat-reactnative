{
  "expo": {
    "name": "Chatio",
    "slug": "chatio",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Chatio is a messaging app from Venny. We believe that enabling users to share ideas with each other is a cornerstone of modern digital experiences.",
    "githubUrl": "https://github.com/withvenny/venny-apps-chatio-reactnative"
  }
}
